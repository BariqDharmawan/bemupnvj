<div class="tab-pane fade {{ isActive ? 'show active' : '' }} {{ addClass || '' }}"
id="tab-{{ dashCase(tabName).replace(/[^a-zA-Z0-9]/g, '') }}" role="tabpanel"
aria-labelledby="tab-{{ dashCase(tabName).replace(/[^a-zA-Z0-9]/g, '') }}-tab"
{{ $props.serializeExcept(['tabName', 'id', 'isActive']) }}>
    {{{ await $slots.main() }}}
</div>
